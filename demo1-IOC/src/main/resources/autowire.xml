    <!-- 自动装配autowire
        byName:根据属性名称注入，即bean的id值应于属性名称一致
        byType:根据属性类型注入
        -->
    <bean id="emp" class="autowire.Emp" autowire="byName"></bean>
    <bean id="dept" class="autowire.Dept"></bean>

</beans>